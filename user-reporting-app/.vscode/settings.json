{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#c5a0ef",
    "activityBar.background": "#c5a0ef",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#a45b1c",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#c5a0ef",
    "sideBar.border": "#c5a0ef",
    "titleBar.activeBackground": "#ab74e8",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#ab74e899",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#ab74e8",
  "editor.defaultFormatter": "biomejs.biome",
  "CSSNavigation.enableIdAndClassNameCompletion": false,
  "CSSNavigation.alwaysIncludeGlobPatterns": [
    "node_modules/@angular/material/prebuilt-themes/rose-red.css",
    "node_modules/bootstrap/dist/css/bootstrap-utilities.css"
  ],
  "css.styleSheets": [
    "**/${fileBasenameNoExtension}.css",
    "**/${fileBasenameNoExtension}.scss"
  ],
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "angular-schematics.schematicsDefaultOptions": {
    "angular-*": {
      "style": "scss"
    },
    "angular-component": {
      "suffix": "component",
      "fileNameSuffix": ".component"
    },
    "angular-directive": {
      "suffix": "directive",
      "fileNameSuffix": ".directive"
    },
    "angular-service": {
      "suffix": "service",
      "fileNameSuffix": ".service"
    },
    "angular-pipe": {
      "fileNameSuffix": ".pipe"
    },
    "angular-page": {
      "fileNameSuffix": ".page"
    },
    "angular-interceptor": {
      "fileNameSuffix": ".interceptor"
    },
    "angular-guard": {
      "fileNameSuffix": ".guard"
    },
    "angular-resolver": {
      "fileNameSuffix": ".resolver"
    },
    "angular-async-reactive-service": {
      "fileNameSuffix": ".service"
    },
    "angular-sync-reactive-service": {
      "fileNameSuffix": ".service"
    },
    "angular-material-dialog": {
      "fileNameSuffix": ".dialog"
    },
    "angular-material-table": {
      "suffix": "component",
      "fileNameSuffix": ".component"
    }
  },
}